        <ImplicitUsings>enable</ImplicitUsings>
        <UserSecretsId>aspnet-PlayDotNet-8621A7E9-3AF6-4600-82CA-6EA9EFCFD178</UserSecretsId>
        <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
    </PropertyGroup>

    <ItemGroup>
